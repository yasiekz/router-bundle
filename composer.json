{
    "name": "yasiekz/router-bundle",
    "description": "Bundle that provides aviability of generating URL address to objects instead of giving route name and route parameters",
    "keywords": ["yasiekz", "Symfony", "routing"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jan Zieba",
            "email": "yasiekz@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.4",
        "symfony/finder": ">=2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "jakub-onderka/php-parallel-lint":  "~0.8"
    },
    "autoload": {
        "psr-4": {
            "Yasiekz\\RouterBundle\\": ""
        }
    },
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "./bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9999-dev",
            "dev-develop": "1.0-dev"
        }
    }
}
